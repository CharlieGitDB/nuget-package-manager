{
  "name": "nuget-package-manager",
  "displayName": "Nuget Package Manager",
  "description": "Extension for Visual Studio code that lets you add or remove packages from .csproj files",
  "version": "1.0.1",
  "repository": {
    "url": "https://github.com/CharlieGitDB/nuget-package-manager"
  },
  "license": "GNU GENERAL PUBLIC LICENSE",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nuget-package-manager.addNugetPackage",
    "onCommand:nuget-package-manager.removeNugetPackage",
    "onCommand:nuget-package-manager.contextMenuAddNugetPackage"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nuget-package-manager.addNugetPackage",
        "title": "Nuget Package Manager: Add Package"
      },
      {
        "command": "nuget-package-manager.removeNugetPackage",
        "title": "Nuget Package Manager: Remove Package"
      },
      {
        "command": "nuget-package-manager.contextMenuAddNugetPackage",
        "title": "Add Package"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "nuget-package-manager.contextMenuAddNugetPackage",
          "group": "7_modification",
          "when": "resourceExtname == .csproj"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/child-process-promise": "^2.2.2",
    "@types/glob": "^7.2.0",
    "@types/got": "^9.6.12",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "child-process-promise": "^2.2.1",
    "got": "^12.5.1"
  }
}
